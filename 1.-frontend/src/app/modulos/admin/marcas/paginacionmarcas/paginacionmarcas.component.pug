mat-table(mat-table, [dataSource]="marcas.items", class="mat-elevation-z8")
    ng-container(matColumnDef="nombre")
        mat-header-cell( *matHeaderCellDef) Nombre
        mat-cell( *matCellDef="let element")
            button(mat-button, (click)="ir(element)") {{element.nombre}}
    mat-header-row( *matHeaderRowDef="['nombre']")
    mat-row(*matRowDef="let row; columns: ['nombre'];")
mat-paginator([length]="marcas.paginas * filtro.limite", [pageSize]="10", [pageSizeOptions]="[5, 10, 25, 100]", (page)="cambioPagina($event)")

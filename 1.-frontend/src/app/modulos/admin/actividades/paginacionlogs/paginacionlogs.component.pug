
mat-card
    mat-table(mat-table, [dataSource]="logs.items", class="mat-elevation-z8")
        ng-container(matColumnDef="nombre")
            mat-header-cell( *matHeaderCellDef) Nombre
            mat-cell( *matCellDef="let element") {{element.usuario?.nombre}}
        ng-container(matColumnDef="fecha")
            mat-header-cell( *matHeaderCellDef) Fecha
            mat-cell( *matCellDef="let element") {{element.createdAt | date:'medium' }}
        mat-header-row( *matHeaderRowDef="['nombre', 'fecha']")
        mat-row(*matRowDef="let row; columns: ['nombre', 'fecha'];")
    mat-paginator([length]="logs.paginas * filtro.limite", [pageSize]="10", [pageSizeOptions]="[5, 10, 25, 100]", (page)="cambioPagina($event)")
